{
  "name": "react-starter-universal",
  "version": "1.0.0",
  "description": "React Redux Starter Unviersal",
  "scripts": {
    "start-dev": "cd build && cross-env NODE_ENV=development && nodemon server.js",
    "build-dev": "cross-env NODE_ENV=development webpack --progress --colors --config webpack.dev.config.js",
    "build-dev-watch": "cross-env NODE_ENV=development webpack --progress --colors --watch --config webpack.dev.config.js",
    "dev": "npm run build-dev && concurrently --kill-others \"npm run build-dev-watch\" \"npm run start-dev\"",
    "start-prod": "cd build && cross-env NODE_ENV=production && node server.js",
    "build-prod": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.prod.config.js",
    "prod": "npm run build-prod && npm run start-prod",
    "test": "cross-env NODE_ENV=test karma start karma.conf.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0",
    "immutable": "^3.8.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-intl": "^2.1.5",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-snippets": "0.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "jasmine": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "nodemon": "^1.11.0",
    "offline-plugin": "^4.5.3",
    "postcss-css-variables": "^0.6.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.0.0",
    "postcss-loader": "^1.2.1",
    "postcss-reporter": "^3.0.0",
    "postcss-url": "^5.1.2",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.4.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-node-externals": "^1.5.4"
  }
}
