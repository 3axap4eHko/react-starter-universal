{
  "name": "react-starter-universal",
  "version": "1.0.0",
  "description": "React Redux Starter Unviersal Application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3axap4eHko/react-starter-universal.git"
  },
  "bugs": {
    "url": "https://github.com/3axap4eHko/react-starter-universal/issues"
  },
  "homepage": "https://github.com/3axap4eHko/react-starter-universal#readme",
  "scripts": {
    "start-dev": "cross-env NODE_ENV=development && nodemon build/server.js",
    "build-dev": "cross-env NODE_ENV=development webpack --progress --colors",
    "build-dev-watch": "cross-env NODE_ENV=development webpack --progress --colors --watch",
    "dev": "yarn build-dev && concurrently --kill-others \"yarn build-dev-watch\" \"yarn start-dev\"",
    "start-prod": "cross-env NODE_ENV=production && node build/server.js",
    "build-prod": "cross-env NODE_ENV=production webpack --progress --colors",
    "prod": "npm run build-prod && npm run start-prod",
    "test": "cross-env NODE_ENV=development karma start karma.conf.js",
    "lint": "eslint src/ test/ webpack/webpack.hot-reload.jsx --ext .js,.jsx",
    "lint:fix": "eslint src/ test/ webpack/webpack.hot-reload.jsx --ext .js,.jsx --fix"
  },
  "keywords": [],
  "author": "Ivan Zakharchenko <3axap4eHko@gmail.com>",
  "license": "MIT",
