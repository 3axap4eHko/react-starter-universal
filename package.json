{
  "name": "react-starter-universal",
  "version": "1.0.0",
  "description": "React Redux Starter Unviersal Application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3axap4eHko/react-starter-universal.git"
  },
  "bugs": {
    "url": "https://github.com/3axap4eHko/react-starter-universal/issues"
  },
  "homepage": "https://github.com/3axap4eHko/react-starter-universal#readme",
  "scripts": {
    "start-dev": "cross-env NODE_ENV=development && nodemon build/server.js",
    "build-dev": "cross-env NODE_ENV=development webpack --progress --colors",
    "build-dev-watch": "cross-env NODE_ENV=development webpack --progress --colors --watch",
    "dev": "concurrently --kill-others \"yarn build-dev-watch\" \"await exists build/server.js && yarn start-dev\"",
    "start-prod": "cross-env NODE_ENV=production && node build/server.js",
    "build-prod": "cross-env NODE_ENV=production webpack --progress --colors",
    "build": "npm run build-prod",
    "prod": "npm run build-prod && npm run start-prod",
    "test": "cross-env NODE_ENV=development jest",
    "lint": "eslint src/ test/ webpack/webpack.hot-reload.jsx --ext .js,.jsx",
    "lint:fix": "eslint src/ test/ webpack/webpack.hot-reload.jsx --ext .js,.jsx --fix"
  },
  "keywords": [],
  "author": "Ivan Zakharchenko <3axap4eHko@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "clean-css": "^4.1.9",
    "compression": "^1.7.1",
    "express": "^4.15.4",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.5.8",
    "react": "^16.0.0-rc.3",
    "react-dom": "^16.0.0-rc.3",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.2.1",
    "react-redux": "^5.0.6",
    "react-steersman": "^1.2.0",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "artillery": "^1.6.0-10",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^4.1.2",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "del": "^3.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.7.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "heapdump": "^0.3.9",
    "jest": "^22.1.4",
    "json-loader": "^0.5.7",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.11.0",
    "null-loader": "^0.1.1",
